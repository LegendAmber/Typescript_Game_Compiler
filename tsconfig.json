{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./Dist",
    "rootDir": "./Typescript Source Dist",
    "allowSyntheticDefaultImports": true,
    "importHelpers": true
  },
  "include": ["Typescript Source Dist/**/*"]
}