{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./mainProj/dist",
    "rootDir": "./mainProj",
    "allowSyntheticDefaultImports": true,
    "importHelpers": true
  },
  "include": ["mainProj/**/*"]
}